{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@mixins/*": [
        "src/mixins/*"
      ],
      "@css/*": [
        "src/css/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@boot/*": [
        "src/boot/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    },
    "allowImportingTsExtensions": true
  }
}